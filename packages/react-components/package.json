{
  "name": "gbif-react-components",
  "version": "1.0.0",
  "description": "React components for use in GBIFs coming websites",
  "module": "./build/esm/index.js",
  "main": "./build/cjs/index.cjs",
  "browser": "./dist/lib/gbif-react-components.js",
  "exports": {
    "import": "./build/esm/index.js",
    "default": "./build/cjs/index.cjs"
  },
  "scripts": {
    "build": "npx webpack --mode=production",
    "build-rollup": "NODE_ENV=production rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve-translations": "npx serve locales/dist/ -p 5002 --cors",
    "watch-translations": "npx nodemon locales/build.js -w locales/ -i locales/dist/",
    "start": "node locales/build.js; start-storybook -p 6006;",
    "build-storybook": "npm run build-locales; build-storybook -o ./dist",
    "build-client": "npm run build-locales; npm run build-storybook; npm run build; npx gulp; npx gzipper compress --brotli --include js,css,html,txt,json,map ./dist",
    "build-locales": "node locales/build",
    "generate": "npx plop --plopfile ./tools/plopfile.js",
    "add-component": "npm run generate component",
    "add-range-filter": "npm run generate rangeFilter",
    "add-enum-filter": "npm run generate enumFilter",
    "add-suggestKey-filter": "npm run generate suggestKeyFilter",
    "add-suggest-filter": "npm run generate suggestFilter",
    "add-text-filter": "npm run generate textFilter"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.1.5",
    "autolinker": "^3.14.3",
    "axios": "^0.21.4",
    "d3": "^7.1.1",
    "doi-regex": "^0.1.10",
    "dompurify": "^2.2.7",
    "downshift": "^6.1.0",
    "fast-deep-equal": "^3.1.3",
    "fast-json-stable-stringify": "^2.1.0",
    "history": "^5.0.0",
    "js-base64": "^3.6.0",
    "latlon-geohash": "^2.0.0",
    "mapbox-gl": "^2.1.1",
    "match-sorter": "^6.3.0",
    "nanoid": "^3.1.22",
    "object-hash": "^2.1.1",
    "query-string": "^7.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-fast-compare": "^3.2.0",
    "react-icons": "^4.2.0",
    "react-intl": "^5.13.4",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "react-use": "^17.2.1",
    "reakit": "^1.3.6",
    "uncontrollable": "^7.2.1",
    "url-regex": "^5.0.0",
    "use-debounce": "^6.0.0",
    "use-query-params": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@storybook/addon-a11y": "^6.1.21",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addon-storysource": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "flat": "^5.0.2",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-replace": "^1.0.0",
    "md5-file": "^5.0.0",
    "nodemon": "^2.0.12",
    "plop": "^2.7.4",
    "pseudoloc": "^1.1.0",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.42.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
